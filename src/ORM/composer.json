{
    "name": "cakephp/orm",
    "description": "CakePHP ORM - Provides a flexible and powerful ORM implementing a data-mapper pattern.",
    "type": "library",
    "keywords": [
        "cakephp",
        "orm",
        "data-mapper",
        "data-mapper pattern"
    ],
    "homepage": "https://cakephp.org",
    "license": "MIT",
    "authors": [
        {
            "name": "CakePHP Community",
            "homepage": "https://github.com/cakephp/orm/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/cakephp/cakephp/issues",
        "forum": "https://stackoverflow.com/tags/cakephp",
        "irc": "irc://irc.freenode.org/cakephp",
        "source": "https://github.com/cakephp/orm"
    },
    "require": {
        "php": ">=8.1",
        "cakephp/collection": "^5.0",
        "cakephp/core": "^5.0",
        "cakephp/datasource": "^5.0",
        "cakephp/database": "^5.0",
        "cakephp/event": "^5.0",
        "cakephp/utility": "^5.0",
        "cakephp/validation": "^5.0"
    },
    "suggest": {
        "cakephp/cache": "If you decide to use Query caching.",
        "cakephp/i18n": "If you are using Translate/TimestampBehavior or Chronos types."
    },
    "autoload": {
        "psr-4": {
            "Cake\\ORM\\": "."
        },
        "files": [
            "bootstrap.php"
        ]
    },
    "require-dev": {
        "cakephp/cache": "^5.0",
        "cakephp/i18n": "^5.0",
        "phpstan/phpstan": "^1.10"
    }
}
